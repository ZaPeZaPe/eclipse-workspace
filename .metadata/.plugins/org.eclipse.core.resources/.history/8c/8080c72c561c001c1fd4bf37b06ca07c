package teste;

public class conta {
	private String nConta;
	private String nome;
	
	public void setNome(String pessoa) {
		this.nome = pessoa;
	}
	
	public void setNumeroConta(int val) {
		String str = String.format("%04d", val);
		this.nConta = str;
	}
	
	public String getNumeroConta() {
		return nConta;
	}
	
	public String getNome() {
		return nome;
	}
}
