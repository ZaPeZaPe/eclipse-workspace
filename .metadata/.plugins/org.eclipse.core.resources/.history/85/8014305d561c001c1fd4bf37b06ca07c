package teste;

import java.util.*;

public class teste {

	public static void main(String[] args) {
		
		conta[] contas = new conta[9998];
		
		contas[0] = new conta(1,"Isaac");
		contas[1] = new conta(2,"Jorge");
		
		System.out.println(contas[0].nome);
		System.out.println(contas[1].nome);
	}

}
